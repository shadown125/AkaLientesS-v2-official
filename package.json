{
  "name": "akalientess-v2-official",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:build": "npm-run-all --parallel copy:* build:*",
    "build:styles": "npm-run-all sass:build postcss:build",
    "build:images": "imagemin src/pics/*.{jpg,png} --out-dir=public",
    "copy:fonts": "copyfiles src/fonts/**/* public --flat",
    "watch": "npm-run-all --parallel watch:*",
    "watch:styles": "npm-run-all --parallel sass:watch postcss:watch",
    "sass:build": "sass src/styles:public --no-source-map",
    "sass:watch": "sass src/styles:src/build --no-source-map --watch",
    "postcss:build": "postcss public/*.css --replace --env production",
    "postcss:watch": "postcss src/build/*.css --watch --dir public"
  },
  "dependencies": {
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.3.5",
    "react-tsparticles": "^2.1.3",
    "sharp": "^0.30.7",
    "tsparticles": "^2.1.3",
    "typewriter-effect": "^2.19.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.12",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-csso": "^6.0.0",
    "sass": "^1.52.3",
    "svgo": "^2.8.0",
    "typescript": "4.7.4"
  }
}
